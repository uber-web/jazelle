#!/bin/bash
BIN=$(dirname "$0")
case $(uname -m) in
  arm*)
    case $OSTYPE in
      darwin*) "$BIN/bazelisk-darwin-arm64" "$@" ;;
      *) "$BIN/bazelisk-linux-arm64" "$@" ;;
    esac;;
  *)
    case $OSTYPE in
      darwin*) "$BIN/bazelisk-darwin-amd64" "$@" ;;
      linux*) "$BIN/bazelisk-linux-amd64" "$@" ;;
      *) "$BIN/bazelisk-windows-amd64.exe" "$@" ;;
    esac;;
esac
